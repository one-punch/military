    <![endif]-->
  </head>

  <body>
    <%= render 'layouts/nav/admin_nav' %>
    <div class="container-fluid">
      <div class="row">
        <%= render 'layouts/nav/admin_sidebar' %>

        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
